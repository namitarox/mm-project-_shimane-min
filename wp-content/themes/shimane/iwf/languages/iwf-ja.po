msgid "The field :label is required and must contain value."
msgstr ":labelは必須入力です。"

#: ../iwf-validation.php:440
msgid "The valid string rule :rule(:param:1) failed for field :label."
msgstr ":labelは正しくない文字列です。（:param:1）"

#: ../iwf-validation.php:441
msgid "The field :label must contain a valid email address."
msgstr ":labelは正しいメールアドレスの形式で入力してください。"

#: ../iwf-validation.php:442
msgid "The field :label must contain a valid URL."
msgstr ":labelは正しいURLの形式で入力してください。"

#: ../iwf-validation.php:443
msgid "The field :label may not contain more than :param:1 characters."
msgstr ":labelは:param:1文字以上で入力してください。"

#: ../iwf-validation.php:444
msgid "The field :label has to contain at least :param:1 characters."
msgstr ":labelは:param:1文字以内で入力してください。"

#: ../iwf-validation.php:445
msgid "The field :label must equal :param:1 characters."
msgstr ":labelは:param:1文字で入力してください。"

#: ../iwf-validation.php:446
msgid "The minimum numeric value of :label must be :param:1"
msgstr ":labelは:param:1以上の値を入力してください。"

#: ../iwf-validation.php:447
msgid "The maximum numeric value of :label must be :param:1"
msgstr ":labelは:param:1以下の値を入力してください。"

#: ../iwf-validation.php:448
msgid "The value of :label must be integer."
msgstr ":labelは整数で入力して下さい。"

#: ../iwf-validation.php:449
msgid "The value of :label must be decimal."
msgstr ":labelは小数で入力して下さい。"

#: ../iwf-validation.php:450
msgid "The field :label must contain the value :param:1."
msgstr ":labelは:param:1と一致しません。"

#: ../iwf-validation.php:451
msgid "The field :label must match the pattern :param:1."
msgstr ":labelは:param:1のパターンにマッチしていません。"

